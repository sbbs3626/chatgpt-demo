    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.36.0",
    "http-server": "^14.1.1",
    "lint-staged": "^13.2.0",
    "npm-run-all": "^4.1.5",
    "plop": "^3.1.2",
    "postcss-html": "^1.5.0",
    "postcss-px2vp": "^1.1.4",
    "sass": "^1.60.0",
    "simple-git-hooks": "^2.8.1",
    "stylelint": "^15.3.0",
    "stylelint-config-standard": "^31.0.0",
    "stylelint-config-standard-scss": "^7.0.1",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-scss": "^4.6.0",
    "svgo": "^3.0.2",
    "typescript": "^5.0.2",
    "unocss": "^0.50.6",
