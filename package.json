    "highlight.js": "^11.7.0",
    "js-cookie": "^3.0.1",
    "mitt": "^3.0.0",
    "mockjs": "^1.1.0",
    "nprogress": "^0.2.0",
    "pinia": "^2.0.33",
    "qs": "^6.11.1",
    "vconsole": "^3.15.0",
    "vue": "^3.2.47",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.37.0",
    "@types/js-cookie": "^3.0.3",
